#!/usr/bin/env perl
use ModelSEED::App::model;
use ModelSEED::Exceptions;
use Try::Tiny;
try {
    ModelSEED::App::model->run; 
} catch {
    local $@ = $_;
    if ( my $e = Exception::Class->caught('ModelSEED::Exception::CLI') ) {
        warn $e->cli_error_text();
    } else {
        warn $@;
    }
};
