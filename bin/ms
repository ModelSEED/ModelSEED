#!/usr/bin/env perl
use strict;
use common::sense;
use ModelSEED::App::mseed;
use ModelSEED::Exceptions;
use Try::Tiny;
try {
    ModelSEED::App::mseed->run; 
} catch {
    local $@ = $_;
    if ( my $e = Exception::Class->caught('ModelSEED::Exception::CLI') ) {
        warn $e->cli_error_text();
    } else {
        warn $@;
    }
};
